{
    "name": "kaishiyoku/strahlungswerte",
    "type": "project",
    "description": "",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "doctrine/dbal": "2.12.1",
        "fideloper/proxy": "4.4.1",
        "fruitcake/laravel-cors": "2.0.3",
        "guzzlehttp/guzzle": "7.2.0",
        "h4kuna/number-format": "3.0.3",
        "kaishiyoku/laravel-cli-create-user": "3.1.0",
        "kaishiyoku/laravel-menu": "4.5.2",
        "kaishiyoku/laravel-recharts": "0.9.0",
        "khill/php-duration": "1.0.6",
        "laracasts/flash": "3.2",
        "laravel/framework": "8.22.1",
        "laravel/helpers": "1.4.0",
        "laravel/horizon": "5.6.0",
        "laravel/tinker": "2.5.0",
        "laravel/ui": "3.1.0",
        "nao-pon/flysystem-google-drive": "1.1.13",
        "predis/predis": "1.1.6",
        "spatie/laravel-backup": "6.13.1",
        "spatie/laravel-html": "2.28.1",
        "tabuna/breadcrumbs": "2.2.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.5.1",
        "barryvdh/laravel-ide-helper": "2.8.1",
        "facade/ignition": "2.5.2",
        "fzaninotto/faker": "1.9.1",
        "laravel-frontend-presets/tailwindcss": "4.3.1",
        "mockery/mockery": "1.4.2",
        "nunomaduro/collision": "5.1.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ide-helper": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --dir=\"app/Models\" --write"
        ]
    }
}
