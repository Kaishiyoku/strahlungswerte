{
    "name": "kaishiyoku/strahlungswerte",
    "type": "project",
    "description": "",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "consoletvs/charts": "6.5.3",
        "cornford/googlmapper": "2.35.1",
        "davejamesmiller/laravel-breadcrumbs": "5.3.2",
        "doctrine/dbal": "2.10.1",
        "fideloper/proxy": "4.2.2",
        "guzzlehttp/guzzle": "6.5.2",
        "kaishiyoku/laravel-cli-create-user": "2.3.2",
        "kaishiyoku/laravel-menu": "3.0.1",
        "khill/php-duration": "1.0.6",
        "laracasts/flash": "3.0.3",
        "laravel/framework": "5.8.36",
        "laravel/horizon": "3.4.6",
        "laravel/tinker": "1.0.10",
        "laravelcollective/html": "5.8.1",
        "predis/predis": "1.1.1",
        "webup/laravel-sendinblue": "2.0.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.2.8",
        "barryvdh/laravel-ide-helper": "2.6.6",
        "beyondcode/laravel-dump-server": "1.3.0",
        "filp/whoops": "2.7.0",
        "fzaninotto/faker": "1.9.1",
        "mockery/mockery": "1.3.1",
        "nunomaduro/collision": "3.0.1",
        "phpunit/phpunit": "7.5.20"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --dir=\"app/Models\" --write"
        ]
    }
}
