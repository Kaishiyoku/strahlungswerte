{
    "name": "kaishiyoku/strahlungswerte",
    "type": "project",
    "description": "",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "blade-ui-kit/blade-heroicons": "^1.2",
        "cjstroud/classnames-php": "^1.0",
        "doctrine/dbal": "^3.1.1",
        "fideloper/proxy": "^4.4.1",
        "fruitcake/laravel-cors": "^2.0.4",
        "guzzlehttp/guzzle": "^7.3.0",
        "h4kuna/number-format": "^3.0.5",
        "kaishiyoku/laravel-cli-create-user": "^3.1.2",
        "khill/php-duration": "^1.1.0",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^8.52.0",
        "laravel/helpers": "^1.4.1",
        "laravel/horizon": "^5.7.9",
        "laravel/tinker": "^2.6.1",
        "laravel/ui": "^3.3.0",
        "nelexa/zip": "^4.0.0",
        "predis/predis": "^1.1.7",
        "ramsey/uuid": "^4.1",
        "spatie/laravel-backup": "^7.6.3",
        "spatie/laravel-html": "^2.30.0",
        "spatie/laravel-sluggable": "^3.1.0",
        "tabuna/breadcrumbs": "^2.3.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6.2",
        "barryvdh/laravel-ide-helper": "^2.10.0",
        "facade/ignition": "^2.11.2",
        "fzaninotto/faker": "^1.9.2",
        "laravel-frontend-presets/tailwindcss": "^4.3.1",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/collision": "^5.6.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --tag=blade-fontawesome --force"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ide-helper": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --dir=\"app/Models\" --write"
        ]
    }
}
