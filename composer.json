{
    "name": "kaishiyoku/strahlungswerte",
    "type": "project",
    "description": "",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "consoletvs/charts": "6.*",
        "davejamesmiller/laravel-breadcrumbs": "5.2.0",
        "doctrine/dbal": "2.8.0",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "kaishiyoku/laravel-cli-create-user": "2.1.0",
        "kaishiyoku/laravel-menu": "2.0.0",
        "khill/php-duration": "1.0.6",
        "laracasts/flash": "3.0.2",
        "laravel/framework": "5.7.15",
        "laravel/tinker": "1.0.8",
        "laravelcollective/html": "5.7.1",
        "webup/laravel-sendinblue": "1.1.3",
        "ext-json": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.2.1",
        "barryvdh/laravel-ide-helper": "2.5.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --dir=\"app/Models\" --write"
        ]
    }
}
